{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "pnpm"
  },
  "newProjectRoot": "projects",
  "projects": {
    "dsx": {
      "projectType": "library",
      "root": "projects/dsx",
      "sourceRoot": "projects/dsx/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/dsx/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/dsx/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/dsx/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/dsx/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        },
        "storybook": {
          "builder": "@storybook/angular:start-storybook",
          "options": {
            "configDir": "projects/dsx/.storybook",
            "browserTarget": "dsx:build",
            "compodoc": true,
            "compodocArgs": [
              "-e",
              "json",
              "-d",
              "projects/dsx"
            ],
            "port": 6006,
            "styles": ["./projects/dsx/src/styles.css"]
          }
        },
        "build-storybook": {
          "builder": "@storybook/angular:build-storybook",
          "options": {
            "configDir": "projects/dsx/.storybook",
            "browserTarget": "dsx:build",
            "compodoc": true,
            "compodocArgs": [
              "-e",
              "json",
              "-d",
              "projects/dsx"
            ],
            "outputDir": "storybook-static",
            "styles": ["./projects/dsx/src/styles.css"]
          }
        }
      }
    }
  }
}